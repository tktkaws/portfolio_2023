<% let path = jsonData.pages[pageId].path %>

<!-- aside -->
<aside class="l-aside"></aside>

<% if (pageId === "home") { %>
<aside class="l-aside__right --home"></aside>
<% } else { %>
<aside class="l-aside__right"></aside>
<% }%>
<!-- aside -->
<!-- footer -->
<fotter class="l-footer">
  <% if (pageId === "home") { %>
    <div class="l-footer__inner --home">
    <% } else { %>
      <div class="l-footer__inner">
    <% }%>


    <div class="l-footer__icons">
      <figure class="l-fotter__icon">
        <a
          href="https://github.com/tktkaws"
          target="_blank"
          rel="noopener noreferrer"
          ><img src="<%= path %>assets/images/common/github.svg" alt="github"
        /></a>
      </figure>
      <figure class="l-fotter__icon">
        <a
          href="https://twitter.com/takaws"
          target="_blank"
          rel="noopener noreferrer"
        >
          <img
            src="<%= path %>assets/images/common/twitter.svg"
            alt="twitter"
          />
        </a>
      </figure>
    </div>

    <small class="l-footer__credit"> Â© Takayuki Takahashi </small>
  </div>
</fotter>
<!-- /footer -->
